{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": " ",
    "foreground": "#d3e6d3"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#d3e6d3",
          "background": "#8a7166",
          "template": "{{.Icon}}",
          "properties": {
            "macos": "\uF179"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{.HostName}} ",
          "foreground": "#243335",
          "background": "#718b62",
          "type": "session",
          "style": "diamond",
          "background_templates": [
            "{{if .Ssh}}#cf9279{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "foreground": "#243335",
          "background": "#aca06d",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "branch_status_icon": "",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}",
          "foreground": "#243335",
          "background": "#719494",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\n",
          "foreground": "#d3e6d3",
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "environment",
            "display_virtual_env_name": true,
            "fetch_version": false
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "home_enabled": true,
          "template": "  {{.Venv}} {{.Full}} ",
          "foreground": "#d3e6d3",
          "background": "#825d4c",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "precision": 0
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uefc5 {{ round .PhysicalPercentUsed .Precision }}% | \uf2db {{round .Load1 .Precision }}% ",
          "foreground": "#d3e6d3",
          "background": "#8a7166",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
