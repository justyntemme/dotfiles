{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": " ",
    "foreground": "#d3e6d3"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#d3e6d3",
          "background": "#8a7166",
          "template": "{{.Icon}}",
          "properties": {
            "macos": "\uF179"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4"
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#111111",
          "background": "#ffff66",
          "template": "ÔÉß"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{.HostName}} ",
          "foreground": "#243335",
          "background": "#718b62",
          "type": "session",
          "style": "diamond",
          "background_templates": [
            ""
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "foreground": "#243335",
          "background": "#825d4c",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ÔÉá {{ .StashCount }}{{ end }}",
          "foreground": "#243335",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#aca06d{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "background": "#719494",
          "type": "git",
          "style": "diamond",
          "mapped_branches": {
            "feat/*": "üöÄ ",
            "bug/*": "üêõ "
          },
          "source": "cli"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\n",
          "foreground": "#d3e6d3",
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#7FD5EA",
          "template": " Óòß {{ .Full }}",
          "properties": {
            "display_mode": "always",
            "home_enabled": true
          }
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "environment",
            "display_virtual_env_name": true,
            "fetch_version": false,
            "home_enabled": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "Û∞å† {{.Venv}} {{.Full}}",
          "foreground": "#d3e6d3",
          "background": "#aca06d",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "precision": 1
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "Óâ¶ {{ round .PhysicalPercentUsed .Precision }}% | \uf2db {{round .Load1 .Precision }}% ",
          "foreground": "#d3e6d3",
          "background": "#8a7166",
          "type": "sysinfo",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
